type Product @key(fields: "id") {
  id: String!
  name: String!
}

type ProductSearchByCriteriaBlob @key(fields: "query") {
  products: [Product!]! @external
  query: String!
}

type Query {
  touches: [Touch!]!
}

type Touch {
  id: String!
  name: String!
  searchCriteria: ProductSearchByCriteriaBlob!
}